
<h3 class="title float-text center-align">  
  <%= @repo.name %>
</h3><br>
 
<section class="container-responsive center-align">
  <p class="left">
    <%= @repo.description %>
  </p>

  <% @commits.each do |commit| %>
  <h5 class="strong">
    <%= commit.author[:name] %>:
  </h5>
  <article class="strong">
    <%= commit.message %>
  </article>
  <% end %>
</section>
